{
  "version": 1,
  "name": "vercel-latency-metrics",
  "description": "Latency metrics collection configuration for Vercel deployment",
  "endpoints": [
    {
      "path": "/api/latency",
      "method": "POST",
      "description": "Endpoint for collecting latency metrics"
    }
  ],
  "monitoring": {
    "enabled": true,
    "metrics": [
      "request_latency",
      "response_time",
      "processing_time"
    ]
  },
  "storage": {
    "type": "log",
    "retention_days": 30
  }
}
